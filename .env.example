# Choose your default LLM provider: ollama, gemini, openai, anthropic
DEFAULT_LLM_PROVIDER=ollama

# Ollama (runs in Docker, no API key needed)
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_PORT=11434
OLLAMA_MODEL_NAME=llama3.2

# Google Gemini (optional, requires API key)
# GEMINI_API_KEY=your_gemini_api_key
# GEMINI_MODEL_NAME=gemini-2.0-flash-001

# OpenAI (optional, requires API key)
# OPENAI_API_KEY=your_openai_api_key
# OPENAI_MODEL_NAME=gpt-4o

# Anthropic (optional, requires API key)
# ANTHROPIC_API_KEY=your_anthropic_api_key
# ANTHROPIC_MODEL_NAME=claude-sonnet-4-20250514

# Choose your embedding provider: ollama, openai, gemini
DEFAULT_EMBEDDING_PROVIDER=ollama

# Ollama embeddings (runs in Docker, auto-pulls nomic-embed-text)
OLLAMA_EMBEDDING_MODEL=nomic-embed-text

# Alternative embedding models (if using cloud providers)
# OPENAI_EMBEDDING_MODEL=text-embedding-3-small
# GEMINI_EMBEDDING_MODEL=text-embedding-004


# PostgreSQL
POSTGRES_USER=support_user
POSTGRES_PASSWORD=change_this_password
POSTGRES_DB=support_chat
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Qdrant (vector database)
QDRANT_HOST=qdrant
QDRANT_HTTP_PORT=6333
QDRANT_GRPC_PORT=6334

BACKEND_PORT=8000
FRONTEND_PORT=3000


# Rate limiting salt (generate a random string)
RATE_LIMIT_SALT=generate_a_random_32_char_string

# Admin API key for document uploads
ADMIN_API_KEY=change_this_admin_key

# App secret key for sessions
SECRET_KEY=change_this_secret_key

CORS_ORIGINS=http://localhost:3000
